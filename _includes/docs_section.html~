{% assign items = include.items %}
{% for item in items %}
  {% assign item_url = item | prepend:"/docs/" | append:"/" %}
  {% if item_url == page.url %}
    {% assign c = "active" %}
  {% else %}
    {% assign c = "" %}
  {% endif %}
  {% for p in site.docs %}
    {% if p.url == item_url %}
      <li class="{{ c }}"><a href="{{ site.url }}{{ p.url }}">{{ p.title }}</a></li>
      {% break %}
    {% endif %}
  {% endfor %}
{% endfor %}
